{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "adftutorialdatafactorymgw"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureDataLakeStore_Source')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "AzureDataLakeStore",
				"typeProperties": {
					"dataLakeStoreUri": "https://adftutorialmgw.azuredatalakestore.net/webhdfs/v1",
					"tenant": "3b46a97a-a951-4816-aebc-0b6c46f1d4e1",
					"subscriptionId": "27dea0bf-804f-40b5-847f-0921ba6a5f0c",
					"resourceGroupName": "adfmw"
				}
			}
		}
	]
}